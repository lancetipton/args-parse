{
  "name": "args-parse",
  "version": "1.0.0",
  "description": "Parse command line arguments",
  "main": "src/index.js",
  "repository": {
    "url": "https://github.com/lancetipton/argsParse.git",
    "type": "git"
  },
  "author": "lancetipton <lancetipton04@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "NODE_ENV=test jest --no-cache",
    "test:report": "yarn test --coverage"
  },
  "files": [
    "src",
    "configs",
    "README.md"
  ],
  "dependencies": {
    "@ltipton/jsutils": "^5.0.0",
    "app-root-path": "^3.0.0",
    "askIt": "git+https://github.com/lancetipton/askIt.git"
  },
  "devDependencies": {
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "jest-html-reporter": "^3.1.3",
    "jest-sonar-reporter": "2.0.0",
    "rimraf": "2.6.3"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "globals": {
      "__DEV__": true
    },
    "moduleNameMapper": {},
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)"
    ],
    "transformIgnorePatterns": [
      ".*"
    ],
   "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/index.{js,jsx,ts,tsx}",
      "!src/**/__mocks__/**/*.{js}",
      "!node_modules/"
    ],
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "ArgsParse Test Results",
          "outputPath": "<rootDir>/reports/test-results.html"
        }
      ]
    ],
    "testURL": "http://localhost/",
    "coverageDirectory": "reports/coverage",
    "setupFilesAfterEnv": []
  }
}

{
  "name": "args-parse",
  "version": "1.0.0",
  "description": "Parse command line arguments",
  "main": "src/index.js",
  "repository": "https://github.com/lancetipton/argsParse.git",
  "author": "lancetipton <lancetipton04@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "NODE_ENV=test jest --no-cache"
  },
  "dependencies": {
    "@ltipton/jsutils": "^5.0.0",
    "app-root-path": "^3.0.0",
    "askIt": "git+https://github.com/lancetipton/askIt.git"
  },
  "devDependencies": {
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "jest-html-reporter": "3.1.3",
    "jest-sonar-reporter": "2.0.0",
    "rimraf": "2.6.3"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "globals": {
      "__DEV__": true
    },
    "moduleNameMapper": {},
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)"
    ],
    "transformIgnorePatterns": [
      ".*"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!**/__mocks__/**/*.{js}"
    ],
    "coverageDirectory": "reports/coverage",
    "setupFilesAfterEnv": []
  }
}
